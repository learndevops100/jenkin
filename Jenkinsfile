pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                echo 'Hello, I am building environment'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello, I am deploying environment'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello, I am testingenvironment'
            }
        }
    }
}
